    "error-subclass": "^2.0.4",
    "handle-quit": "^0.1.0",
    "hoxy": "^3.2.0",
    "is-root": "^1.0.0",
    "meow": "^3.7.0",
    "opn": "^4.0.2",
    "redent": "^2.0.0",
    "require-dir": "^0.3.1",
    "root-check": "^1.0.0",
